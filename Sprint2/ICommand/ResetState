using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Sprint2 
{
    class ResetState: ICommand
    {
        private Game1 myGame;
        public ResetState(Game1 game)
        {
            myGame = game;
        }
        public void Execute()
        {
            myGame.player.ResetState(); 
        }
    }
}
